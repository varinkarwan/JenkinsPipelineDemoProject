pipeline{
agent any
stages 
{
stage('Build') 
{
steps{
echo "Building the Code.........."
bat "npm clean"
}
}
stage('Test') 
{
steps{
echo "Testing the Code.........."
bat "npm test"
}
}
stage('Compile') 
{
steps{
echo "Compiling the Project.........."
bat "npm compile"
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
}
}
}
}
